      }
    },
    "apiVersionNetwork": {
      "type": "string",
      "metadata": {
        "description": "The network api version"
      }
    },
    "apiVersionStorage": {
      "type": "string",
      "metadata": {
        "description": "The storage api version"
      }
    },
    "f5NamePrefix": {
      "type": "string"
    },
