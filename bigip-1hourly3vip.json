                "id": "[parameters('mgmtSubnetRef')]"
              },
              "publicIPAddress": {
                "id": "[resourceId('Microsoft.Network/publicIPAddresses', concat(parameters('publicIPName'),'-mgmt',copyIndex(1)))]"
