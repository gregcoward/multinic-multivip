      "type": "string"
    },
    "extIpAddress": {
      "type": "string"
    },
    "extSubnetRef": {
      "type": "string"
    },
    "nicNamePrefix": {
      "type": "string"
    },
    "nsgID": {
      "type": "string"
    },
    "numberOfInstances": {
      "type": "int"
    },
    "numCopies": {
      "type": "int"
    },
    "loadBalancerName": {
      "type": "string"
    },
    "imageName": {
      "type": "string"
    },
    "licensedBandwidth": {
      "type": "string"
    },
    "virtualNetworkName": {
      "type": "string"
    },
    "availabilitySetName": {
      "type": "string"
    },
    "newStorageAccountName": {
      "type": "string"
    },
    "diagStorageAccountName": {
      "type": "string"
    }
